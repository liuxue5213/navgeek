Date.prototype.Format = function(formatStr) <wbr> <wbr> <wbr>
{ <wbr> <wbr> <wbr>
<wbr> <wbr> <wbr> var str = formatStr; <wbr> <wbr> <wbr>
<wbr> <wbr> <wbr> var Week = ['日','一','二','三','四','五','六']; <wbr> <wbr>
<wbr> <wbr>
