//| 求两个时间的天数差 日期格式为 YYYY-MM-dd <wbr> <wbr> <wbr>
//+--------------------------------------------------- <wbr> <wbr>
function daysBetween(DateOne,DateTwo) <wbr> <wbr>
{ <wbr> <wbr> <wbr>
<wbr> <wbr> <wbr> var OneMonth = DateOne.substring(5,DateOne.lastIndexOf ('-')); <wbr> <wbr>
<wbr> <wbr> <wbr> var OneDay = DateOne.substring(DateOne.length,DateOne.lastIndexOf ('-')+1); <wbr> <wbr>
<wbr> <wbr> <wbr> var OneYear = DateOne.substring(0,DateOne.indexOf ('-')); <wbr> <wbr>
<wbr> <wbr>
<wbr> <wbr> <wbr> var TwoMonth = DateTwo.substring(5,DateTwo.lastIndexOf ('-')); <wbr> <wbr>
<wbr> <wbr> <wbr> var TwoDay = DateTwo.substring(DateTwo.length,DateTwo.lastIndexOf ('-')+1); <wbr> <wbr>
<wbr> <wbr> <wbr> var TwoYear = DateTwo.substring(0,DateTwo.indexOf ('-')); <wbr> <wbr>
<wbr> <wbr>
<wbr> <wbr> <wbr> var cha=((Date.parse(OneMonth+'/'+OneDay+'/'+OneYear)- Date.parse(TwoMonth+'/'+TwoDay+'/'+TwoYear))/86400000); <wbr> <wbr> <wbr>
<wbr> <wbr> <wbr> return Math.abs(cha); <wbr> <wbr>
} <wbr> <wbr>
